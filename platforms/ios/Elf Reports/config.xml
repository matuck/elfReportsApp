<?xml version='1.0' encoding='utf-8'?>
<widget id="com.matuckcomputing.elfreports" version="0.9.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="FadeSplashScreenDuration" value=".25" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="ShowSplashScreenSpinner" value="true" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="TopActivityIndicator" value="gray" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <feature name="AdMob">
        <param name="ios-package" value="CDVAdMobPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Elf Reports</name>
    <description>
        Elf reports is a great way to see what your elf has been reporting, if you are naughty or nice, and send your Christmas list to Santa.
    </description>
    <icon height="57" platform="ios" src="www/res/img/icon57.png" width="57" />
    <icon height="72" platform="ios" src="www/res/img/icon72.png" width="72" />
    <icon height="76" platform="ios" src="www/res/img/icon76.png" width="76" />
    <icon height="120" platform="ios" src="www/res/img/icon120.png" width="120" />
    <icon height="152" platform="ios" src="www/res/img/icon152.png" width="152" />
    <icon src="www/res/img/icon.png" />
    <icon density="ldpi" platform="android" src="www/res/img/icon.png" />
    <icon density="mdpi" platform="android" src="www/res/img/icon.png" />
    <icon density="hdpi" platform="android" src="www/res/img/icon.png" />
    <icon density="xhdpi" platform="android" src="www/res/img/icon.png" />
    <author email="support@elfreports.com" href="http://www.elfreports.com">
        Matuck Computing
    </author>
    <content src="index.html" />
    <access origin="http://www.elfreports.com/*" />
    <access origin="*googleads.g.doubleclick*" />
</widget>
